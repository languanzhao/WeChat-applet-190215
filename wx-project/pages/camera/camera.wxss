.camera{
	display: flex;
	flex-wrap: wrap;
}
.camera image{
	width:100rpx;
	height: 200rpx;
}
.video video{
	width:300rpx;
	height:200rpx;
}
.btn{
	display: flex;
	justify-content: space-around;
	margin:10rpx 0;
}
button:after{
	border: none;
}
button[type=primary]{
	background:#999;
}
button{
	width:200rpx;
	height:60rpx;
	font-size:30rpx;
	border:none;
	border-radius:30rpx;
	padding:0;
	margin:0;
	line-height:60rpx;
}
.nameTitle{
	width: 100%;
	font-size: 30rpx;
	margin-left: 30rpx;
}
.camera,.video{
	display: flex;
	flex-wrap: wrap;
	justify-content: space-around;
}