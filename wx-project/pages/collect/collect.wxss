
.none {
	width: 300rpx;
	height:300rpx;
	margin:0 auto;
	position: absolute;
	top:200rpx;
	left:0rpx;
	right:0rpx;
}
.none .text{
	text-align: center;
	font-size:40rpx;
	color: #666;
	margin-top: 20rpx;
}
.none image{
	display: block;
	width:300rpx;
	height:300rpx;
}
.collectItems{display: flex;flex-wrap: wrap;}
.searchInfo{width:200rpx;height:270rpx;display:flex;flex-direction:column;margin:10rpx 28rpx 10px 10px;}
.searchInfo image{width:200rpx;height:250rpx;}
.searchInfo view{font-size:26rpx;text-align:center;overflow:hidden;white-space: nowrap;text-overflow:ellipsis;}